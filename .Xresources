XTerm*background: Black
XTerm*foreground: gray90
XTerm*cursorColor: grey
XTerm*dynamicColors: on
!XTerm*faceName: Inconsolata
!XTerm*faceSize: 9
!XTerm*font: -*-fixed-medium-r-*-*-13-*-*-*-*-*-iso10646-*

XTerm*geometry: 80x25
XTerm*scrollBar: false
XTerm*charClass:33:48,35:48,37-38:48,45-47:48,58:48,61:48,63-64:48,126:48
XTerm*backarrowKeyIsErase: false
XTerm*locale: UTF-8
XTerm*utf8: 1
XTerm*eightBitInput: false
XTerm*termName: xterm-256color
XTerm*saveLines: 4096
XTerm*bellIsUrgent: true

URxvt.reverseVideo: true
URxvt.scrollBar: false
URxvt.saveLines: 10000
URxvt.fading: 10
URxvt.visualBell: false
URxvt.font: xft:terminus:pixelsize=12,xft:unifont
URxvt.urlLauncher: google-chrome
URxvt.underlineURLs: true
!URxvt.perl-lib: /home/davean/.urxvt-perl/
!URxvt.perl-ext-common: url-select
URxvt.perl-ext-common: bell-command
URxvt.bell-command: aplay -q ~/etc/sounds/beep1.wav
URxvt.urgentOnBell: true
